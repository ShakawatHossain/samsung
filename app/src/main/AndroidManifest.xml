<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gnt.samsung.app" >

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FiledownActivity"
            android:label="@string/title_activity_filedown" >
        </activity>
        <activity
            android:name=".AutoRecieve"
            android:label="@string/title_activity_auto_recieve" >
        </activity>
        <activity
            android:name=".OutGoingCallActivity"
            android:label="@string/title_activity_out_going_call" >
        </activity>
        <activity
            android:name=".FileUploadActivity"
            android:label="@string/title_activity_file_upload" >
        </activity>

        <service
            android:name=".MyLocation"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
